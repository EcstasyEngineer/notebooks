    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Sync Stronny (no whisper)",
            "type": "debugpy",
            "request": "launch",
            "program": "youtube/scrape_chats.py",
            "cwd": "youtube/",
            "console": "integratedTerminal",
            "args": ["@StronnyCuttles"]
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ]
        }
    ]
}